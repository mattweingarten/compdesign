int ack(int m, int n) {
    if (m == 0) {
        return n + 1;
    } 

    if (m > 0 & n == 0) {
        return ack(m - 1, 1);
    }

    if (m > 0 & n > 0) {
        return ack(m - 1, ack(m, n-1));
    }
    return -1;
}

int program (int argc, string[] argv) {
    return ack(3, 4);
}
